{
  "extends": "@antfu",
  "rules": {
    "comma-dangle": "off",
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "warn",
    "no-console": "warn",
    "space-before-function-paren": "off",
    "brace-style": [
      "error",
      "1tbs"
    ]
    // "brace-style": "1tbs"
  },
  "ignorePatterns": [
    "node_modules/**",
    "dist/**"
  ]
}
